<SDRun xmlns:xi="http://www.w3.org/2001/XInclude" xmlns="http://stochdiff.textensor.org">
    <InitialConditions>
  <ConcentrationSet>
    <NanoMolarity specieID="RyR3" value="35.000000"/>
    <NanoMolarity specieID="Ca" value="0.000000"/>
  </ConcentrationSet>
</InitialConditions>
    <Morphology>

    <Segment id="seg1" region="dend">
        <start x="0.0" y="0.0" z="0.0" r="1"/>
        <end x="1.0" y="0.0" z="0.0" r="1" label="pointA"/>
    </Segment>

</Morphology>
    <ReactionScheme>
  <Specie name="Ca" id="Ca" kdiff="100" kdiffunit="mu2/s"/>
  <Specie name="CaER" id="CaER" kdiff="10." kdiffunit="mu2/s"/>
  <Specie name="RyR3" id="RyR3" kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="RyR3C1" id="RyR3C1" kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="RyR3C2" id="RyR3C2" kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="RyR3C3" id="RyR3C3" kdiff="0" kdiffunit="mu2/s"/>  
  <Specie name="RyR3O1" id="RyR3O1" kdiff="0" kdiffunit="mu2/s"/>
  <Specie name="RyR3O2" id="RyR3O2" kdiff="0" kdiffunit="mu2/s"/>
<!-- Stochastic model for correct transitions need K=ker/kef= 0.001uM^4
     Keizer and Smith -->


 <Reaction name="RyR3e" id="RyR3e">
    <Reactant specieID="RyR3"/>
    <Reactant specieID="Ca" power="4"/>
    <Product specieID="RyR3C1"/>
    <forwardRate>9.596034332373223e-09</forwardRate>
    <reverseRate>0.005554006279977181</reverseRate>
 </Reaction>

  <Reaction name="RyR3a" id="RyR3a">
    <Reactant specieID="RyR3C1"/>
    <Reactant specieID="Ca" power="4"/>
    <Product specieID="RyR3O1"/>
    <forwardRate>9.771683263916449e-09</forwardRate>
    <reverseRate>0.11353170452004681</reverseRate>
 </Reaction>
   

  <Reaction name="RyR3b" id="RyR3b">
    <Reactant specieID="RyR3O1"/>
    <Reactant specieID="Ca" power="4"/>
    <Product specieID="RyR3O2"/>
    <forwardRate>7.482778250732669e-16</forwardRate>
    <reverseRate>75.32274966863659</reverseRate>
  </Reaction>

   <Reaction name="RyR3c" id="RyR3c">
    <Reactant specieID="RyR3C2"/>
    <Reactant specieID="Ca" power="4"/>
    <Product specieID="RyR3O2"/>
    <forwardRate>7.482778250732669e-14</forwardRate>
    <reverseRate>7.482778250732669e-14</reverseRate>
  </Reaction>

  <Reaction name="RyR3d" id="RyR3d">
    <Reactant specieID="RyR3C1"/>
    <Reactant specieID="Ca" power="4"/>
    <Product specieID="RyR3C2"/>
    <forwardRate>9.77168326391645e-11</forwardRate>
    <reverseRate>0.0011353170452004682</reverseRate>
  </Reaction>

  <Reaction name="RyR3f" id="RyR3f">
    <Reactant specieID="RyR3C2"/>
    <Reactant specieID="Ca" power="4"/>
    <Product specieID="RyR3C3"/>
    <forwardRate>3.905203815036671e-17</forwardRate>
    <reverseRate>0.10378980249995655</reverseRate>
 </Reaction>
    
</ReactionScheme>

    <!--2D means the morphology is interpreted like a flatworm, 3D for
roundworms. The 2D case is good for testing as it is easy to visualize the
results (also, 3D may not work yet...)  -->
   
    <geometry>          2D           </geometry>
    <depth2D>           10          </depth2D>
    <distribution>      BINOMIAL     </distribution>
    <algorithm>         INDEPENDENT  </algorithm>
    <simulationSeed>    245         </simulationSeed>
    <outputQuantity>NUMBER</outputQuantity>

    <!-- run time for the calculation, milliseconds -->
    <runtime>5000</runtime>

    <!-- set the seed to get the same spines each time testing -->
    <spineSeed>123</spineSeed>

    <discretization>
      <defaultMaxElementSide>10</defaultMaxElementSide>
      <surfaceLayers>10</surfaceLayers> 
    </discretization>
    <tolerance>0.01</tolerance>

    <outputInterval>1</outputInterval>

    <calculation>GRID_ADAPTIVE</calculation>

</SDRun>